<h3 class="text-center mt-5">パスワードリセットフォーム</h3>
<div class="container pb-5">
 <%= form_with scope: :password_resets, url: password_resets_path, local: true ,method: :post  do |form| %>
  <div class="form-wrapper">
    <div class="form-group">
      <p class="text-center">メールアドレス</p>
      <%= form.text_field :email, class:"form-control"%>
    </div>
  <%= form.submit  value="メール送信" ,class:"btn btn-primary"%>
    </div>
  <%end%>
</div>
